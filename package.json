{
  "name": "postgresql-with-exprss-starter-templete",
  "version": "1.0.0",
  "description": "Storefront is a Node & Postgres db backend for handmade ecommerce project that's enable users to see handmade products and make orders",
  "main": "index.js",
  "scripts": {
    "prewatch": "tsc",
    "watch": "tsc-watch --esModuleInterop src/index.ts --outDir ./dist --onSuccess \"node ./dist/index.js\"",
    "prestart": "tsc",
    "start": "node dist/",
    "migrt": "db-migrate create getCompletedOrderCount-function --sql-file",
    "up": "db-migrate up --e test",
    "down": "db-migrate down --e test",
    "reset": "db-migrate reset --e test",
    "dev:up": "db-migrate up ",
    "dev:down": "db-migrate down ",
    "dev:reset": "db-migrate reset ",
    "pretest": "tsc",
    "test": "npm run reset&&set ENV=test&& npm run up && jasmine && npm run reset"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/morgan": "^1.9.4",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jasmine": "^4.3.1",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0"
  }
}
